generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       =  env("DATABASE_URL")
}

model Product {
  id          Int      @id @default(autoincrement())
  name        String
  price       Int
  weight      Int
  description String
  ingridients String
  category Category @relation(fields: [categoryId], references: [id])
  categoryId Int
}

model Category {
  id Int @id @default(autoincrement())
  name String @unique
  products Product[]
}
